# Database Configuration
PGUSER=
PGPASSWORD=
PG_URL=

# FalkorDB Configuration
FALKORDB_HOST=localhost
FALKORDB_PORT=6379
FALKORDB_USERNAME=
FALKORDB_PASSWORD=

# AI Model API Keys
# OpenAI (optional - for embeddings/fallback)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_BASE=https://api.openai.com/v1

# Google AI (primary for agents)
# Get your API key from: https://ai.google.dev/gemini-api/docs/api-key
GOOGLE_API_KEY=
# Gemini models - see https://ai.google.dev/gemini-api/docs/models
GEMINI_MODEL=gemini-2.0-flash  # Good for most use-cases, fast and efficient
GEMINI_PRO_MODEL=gemini-2.0-flash-exp  # Stronger model for complex tasks
GEMINI_EMBEDDING_MODEL=text-embedding-004  # Latest Gemini embedding model

# Optional: Use Vertex AI instead of Google AI Studio
# GOOGLE_GENAI_USE_VERTEXAI=true
# GOOGLE_CLOUD_PROJECT=your-gcloud-project-id
# GOOGLE_CLOUD_LOCATION=us-central1

# Anthropic (optional)
ANTHROPIC_API_KEY=

# API Security
API_KEY=
ALLOWED_ORIGINS=http://localhost:8001,http://localhost:3000
DOCS_ENABLED=true

# Data Synchronization
ENABLE_SYNC_SCHEDULER=false
SYNC_INTERVAL_HOURS=24
SYNC_ON_STARTUP=true

# Agent Configuration
AGENT_CACHE_ENABLED=true
AGENT_CACHE_TTL_HOURS=24
AGENT_MAX_RETRIES=3
AGENT_TIMEOUT_SECONDS=300

# Search Configuration
SEARCH_RESULT_LIMIT=10
SEARCH_INCLUDE_RELATIONSHIPS=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Optional: Load sample data on startup (for development)
LOAD_SAMPLE_DATA=false